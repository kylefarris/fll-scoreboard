{
    "name": "fll-gameday-calc",
    "version": "0.0.1",
    "description": "Calculator",
    "contributors": [
        "Clark Winkelmann <clark.winkelmann@gmail.com> (https://clarkwinkelmann.com/)",
        "Kyle Farris <kylefarris@gmail.com> (https://kylefarris.com/)"
    ],
    "license": "MIT",
    "scripts": {
        "development": "NODE_ENV=development npx mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "NODE_ENV=development npx mix watch --hot",
        "production": "mix --production",
        "serve": "python3 -m http.server -d ./site 4000",
        "test": "./node_modules/.bin/karma start karma.conf.js --single-run",
        "push": "aws s3 sync --profile gameday_key_set --exclude='.DS_Store' site/ s3://calc.fllgameday.com"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.10",
        "@types/materialize-css": "^1.0",
        "@types/mithril": "^2.0",
        "material-icons": "^1.13.12",
        "materialize-css": "^1.0",
        "mithril": "^2.0",
        "mithril-materialized": "^1.2.2",
        "sweetalert": "^2.1.2"
    },
    "devDependencies": {
        "@types/jasmine": "^3.4.0",
        "@types/karma": "^3.0.3",
        "cross-env": "^5.2.0",
        "jasmine-core": "^3.4.0",
        "karma": "^6.0",
        "karma-chrome-launcher": "^3.0",
        "karma-jasmine": "^5.0",
        "laravel-mix": "^6.0.43",
        "puppeteer": "^14.0",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.49.9",
        "sass-loader": "^12.1.0",
        "ts-loader": "^9.2.7",
        "typescript": "^4.6.2",
        "vite": "^5.3.1"
    }
}
